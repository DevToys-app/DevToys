@namespace DevToys.Blazor.Components

@inherits JSStyledComponentBase

<CascadingValue Value="this" IsFixed="true">
    <button id=@Id
            role="button"
            class="@(FinalCssClasses)"
            style="@(Style)"
            tabindex="0"
            @ref=Element
            @attributes="AdditionalAttributes">
        @ChildContent
    </button>
</CascadingValue>