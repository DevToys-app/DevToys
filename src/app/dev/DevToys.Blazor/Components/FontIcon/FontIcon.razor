@namespace DevToys.Blazor.Components
@inherits StyledComponentBase

<CascadingValue Value="this" IsFixed="true">
    <i id=@Id
       class="font-icon @(FinalCssClasses)"
       style="@Style"
       @ref=Element
       @attributes="AdditionalAttributes"
       data-glyph='@Glyph'>
    </i>
</CascadingValue>